    <meta property="locale" content="en_US" />
    <meta property="title" content="An English Bed, Breakfast & Day Spa." />
    <meta property="url" content="http://englishbb.com" />
    <meta property="site_name" content="An English Bed, Breakfast & Day Spa." />


    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/ebbcss")
    @Styles.Render("~/Content/owl_carousel_style")

    @Styles.Render("~/Content/jqmstyle_cdn")



    @Scripts.Render("~/bundles/jqmobile_cdn")
    @Scripts.Render("~/bundles/owl_carousel_js")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    <script>

    (function () {

        //        $(document).bind("swiperight", function () {
        //            $.mobile.changePage('at_Url.Action("IndexMobile", "Store")');
        //        });
        //
        //        $(document).bind("swipeleft", function () {
        //            addToList("swipeleft");
        //        });
        //
