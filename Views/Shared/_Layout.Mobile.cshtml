</head>
<body>
    <div data-role="page">
        <div data-role="header">

            <div class="row">
                <div style="background:url(../../images/banner_a.jpg) orange">
                    <div align="center" style="padding:14px">
                        <p>
                            <img class="img-responsive" id="EnglishBB_Title" alt="An English Bed, Breakfast & Day Spa" src="images/logo_english3.gif" />
                        </p>
                        <div align="center">
                            <a href="http://www.powell-graphics4.com/calendar/221/calendar.cgi" class="ui-btn ui-corner-all ui-shadow ui-icon-info ui-btn-icon-right">Availability</a>

                        </div>
                    </div>
                </div>
            </div>

            <div data-role="navbar">
                <ul>
                    <li><a href="@Url.Action("Index", "Home")" data-icon="home">Home</a></li>
                    <li><a href="@Url.Action("Rates", "Home")" data-icon="star">Rates</a></li>
                    <li><a href="@Url.Action("Rooms", "Home")" data-icon="user">Rooms</a></li>
                    <li><a href="@Url.Action("Menus", "Home")" data-icon="bars">Menus</a></li>
                    <li><a href="@Url.Action("Contact", "Home")" data-icon="phone">Contact</a></li>
                </ul>
            </div>
            <div data-role="controlgroup" data-type="vertical">
                @*<p>More Information:</p>*@
                <a href="@Url.Action("Dayspa", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right">Day Spa</a>
                <a href="@Url.Action("Directions", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right">Directions</a>
                <a href="@Url.Action("Links", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right">Links</a>
                <a href="@Url.Action("Gallery", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right">Gallery</a>
                @*<a href="http://www.powell-graphics4.com/calendar/221/calendar.cgi" class="ui-btn ui-corner-all ui-shadow ui-icon-carat-r ui-btn-icon-right">Availability</a>*@

            </div>

            



            @*<ul data-role="listview" data-inset="true">
                    <li>
                        <a href="Rooms"><img src="~/images/nav_rooms_on.gif"></a>
                    </li>
                    <li>
                        <a href="Rates"><img src="~/images/nav_rates_on.gif"></a>
                    </li>
                    <li>
                        <a href="Menus"><img src="~/images/nav_menus_on.gif"></a>
                    </li>
                </ul>*@


        </div>

        <div data-role="content">
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
            <section class="content-wrapper main-content clear-fix">
                @RenderSection("featured", required: false)
            </section>
        </div>

  

        
            <div data-role="controlgroup" data-type="vertical">
                @*<p>More Information:</p>*@
                <a href="@Url.Action("Dayspa", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right ui-mini">Day Spa</a>
                <a href="@Url.Action("Directions", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right ui-mini">Directions</a>
                <a href="@Url.Action("Links", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right ui-mini">Links</a>
                <a href="@Url.Action("Gallery", "Home")" class="ui-btn ui-corner-all ui-icon-carat-r ui-btn-icon-right ui-mini">Gallery</a>
                <a href="http://www.powell-graphics4.com/calendar/221/calendar.cgi" class="ui-btn ui-corner-all ui-shadow ui-icon-carat-r ui-btn-icon-right ui-mini">Availability</a>

            </div>
        
        <div data-role="footer">


            <div data-role="navbar">
                <ul>
                    <li><a href="@Url.Action("Index", "Home")" data-icon="home">Home</a></li>
                    <li><a href="@Url.Action("Rates", "Home")" data-icon="star">Rates</a></li>
                    <li><a href="@Url.Action("Rooms", "Home")" data-icon="user">Rooms</a></li>
                    <li><a href="@Url.Action("Menus", "Home")" data-icon="bars">Menus</a></li>
                    <li><a href="@Url.Action("Contact", "Home")" data-icon="phone">Contact</a></li>
                </ul>
            </div>

            <div class="content-wrapper">
                <div align="center">

                    <p class="small">
                        &copy; @DateTime.Now.Year - An English Bed, Breakfast &amp; Day Spa&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; -  Webmaster: Troy Pells.
                    </p>
                </div>
            </div>
        </div>
    </div>



    @RenderSection("scripts", required: false)
