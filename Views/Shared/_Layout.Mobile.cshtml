    <script>
        $(document).ready(function () {

            //$("#owl-example").owlCarousel();
            //<div id="owl-example" class="owl-carousel">


            //<div id="owl-example" >
            $("#owl-example").owlCarousel({

                autoPlay: 3000, //Set AutoPlay to 3 seconds

                items: 4,
                itemsDesktop: [1199, 3],
                itemsDesktopSmall: [979, 3]

            });


        });


